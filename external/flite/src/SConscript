Import('env')

objs = []
for subdir in ['cg', 'synth', 'wavesynth', 'utils', 'stats', 'speech'
               ,'regex', 'lexicon', 'hrg', 'audio']:
    objs.extend(env.SConscript('%s/SConscript' % subdir, {'env': env}))

env.Library('#/lib/flite', objs)

